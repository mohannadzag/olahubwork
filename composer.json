{
    "name": "olahub/user-code",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "league/fractal": "0.17.*",
        "barryvdh/laravel-cors": "0.11.*",
        "illuminate/mail": "5.5.*",
        "guzzlehttp/guzzle": "6.3.*",
        "stichoza/google-translate-php": "~3.2",
        "illuminate/session": "5.5.*",
        "laravel/scout": "^7.1"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "olahubwork",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/mohannadzag/olahubwork",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "classmap": [
            "src"
        ]
    },
    "autoload-dev": {
        
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    }
}
